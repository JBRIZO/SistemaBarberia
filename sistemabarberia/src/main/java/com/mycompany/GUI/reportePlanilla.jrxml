	<queryString language="SQL">
		<![CDATA[SELECT
     planillas.`IDEmpleado` AS planillas_IDEmpleado,
     planillas.`Periodo` AS planillas_Periodo,
     planillas.`TotalPagar` AS planillas_TotalPagar,
     empleado.`NomEmpleado` AS empleado_NomEmpleado,
     deduccionesempleadomensual.`Valor` AS deduccionesempleadomensual_Valor,
     bonosempleadomensual.`Valor` AS bonosempleadomensual_Valor
FROM
     `empleado` empleado INNER JOIN `planillas` planillas ON empleado.`idempleado` = planillas.`IDEmpleado`
     INNER JOIN `deduccionesempleadomensual` deduccionesempleadomensual ON empleado.`idempleado` = deduccionesempleadomensual.`IDEmpleado`
     INNER JOIN `bonosempleadomensual` bonosempleadomensual ON empleado.`idempleado` = bonosempleadomensual.`IDEmpleado`]]>
